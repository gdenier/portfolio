---
import Layout from "../layouts/Layout.astro"

interface ProjectFrontmatter {
  draft: boolean
  project: {
    name: string
    side: boolean
  }
}

interface Props {
  frontmatter: ProjectFrontmatter
}
const { frontmatter } = Astro.props
---

<Layout title={frontmatter.project.name}>
  <h1>{frontmatter.project.name}</h1>
</Layout>
