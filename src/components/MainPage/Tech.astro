---
export interface TechFrontmatter {
  name: string
  icon: string
  order: number
}

interface Props {
  tech: TechFrontmatter
}
const { tech } = Astro.props
---

<div class="rounded bg-white p-3">
  <p class="sr-only">{tech.name}</p>
  <div class="flex h-full w-6 items-center justify-center">
    <img src={tech.icon} />
  </div>
</div>
